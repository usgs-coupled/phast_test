#
        Fhy_unieOH1.5 = Fhy_unieOH1.5
             -cd_music  0.5 0 0 0 0
             log_k 0

         Fhy_unieOH1.5 = Fhy_unieOH-0.5 + 0.5H+
             -cd_music  0 0 0 0 0
             log_k 20

        Fhy_unieOH-0.5 + H+ = Fhy_unieOH2+0.5
             -cd_music  1 0 0 0 0
             log_k    8.06 #HvR2009

        Fhy_unieOH-0.5 + Na+ = Fhy_unieOHNa+0.5
             -cd_music  0 1 0 0 0
             log_k    -0.60 #HvR2009

        Fhy_unieOH-0.5 + H+ + NO3- = Fhy_unieOH2NO3-0.5
